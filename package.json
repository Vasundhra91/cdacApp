{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3001 && node bin/www && node app.js",
    "server": "set PORT=3001 && nodemon bin/www && nodemon app.js",
    "Dashboard-install": "npm install --prefix Dashboard",
    "Dashboard": "npm start --prefix Dashboard",
    "dev": "concurrently  \"npm run server\" \"npm run Dashboard\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix Dashboard && npm run build --prefix Dashboard"
  
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "gridfs-stream": "^1.1.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.2",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.1.0",
    "nodemon": "^2.0.2",
    "uuid": "^7.0.3"
  },
